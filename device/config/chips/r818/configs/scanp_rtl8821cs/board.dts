 */
		eink: eink@06400000 {
			eink_panel_name     = "default_eink";
			eink_scan_mode      = <0>;
			eink_width          = <1024>;
			eink_height         = <758>;
			eink_bits           = <5>;
			eink_data_len       = <8>;
			eink_lsl            = <6>;
			eink_lbl            = <6>;
			eink_lel            = <38>;
			eink_fsl            = <2>;
			eink_fbl            = <4>;
			eink_fel            = <5>;
			eink_lgonl          = <262>;
			eink_gdck_sta       = <4>;
