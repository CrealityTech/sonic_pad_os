		ctp {
			device_type = "ctp";
			status = "okay";
			ctp_name = "gt9xxnew_ts";
			ctp_twi_id = <0x0>;
			ctp_twi_addr = <0x5D>;
			ctp_screen_max_x = <1024>;
			ctp_screen_max_y = <600>;
			ctp_revert_x_flag = <0x0>;
			ctp_revert_y_flag = <0x0>;
			ctp_exchange_x_y_flag = <0x0>;
			ctp_int_port = <&pio PF 6 6 1 1 0>;
			ctp_wakeup = <&pio PF 1 1 1 1 1>;
			ctp-supply = <&reg_dcdc5>;
			ctp_power_ldo = <&reg_dcdc5>;
			ctp_power_ldo_vol = <3300>;
		};

		wlan: wlan@0 {
			compatible    = "allwinner,sunxi-wlan";
			clocks        = <&clk_losc_out>;
			pinctrl-0;
			pinctrl-names;
			wlan_busnum    = <0x1>;
			wlan_power_num = <1>;
			wlan_power1    = "axp806-dcdce";
			wlan_io_regulator = "axp806-aldo2";
			wlan_io_voltage = <3300000>;
			wlan_power_voltage = <3300000>;
			wlan_regon    = <&r_pio PL 5 1 0x1 0x2 0>;
			wlan_hostwake = <&r_pio PL 6 6 0x1 0x2 0>;
			chip_en;
			power_en;
			status        = "okay";
			wakeup-source;
		};

		bt: bt@0 {
			compatible    = "allwinner,sunxi-bt";
			clocks        = <&clk_losc_out>;
			bt_power_num = <0x01>;
			bt_power1      = "axp806-dcdce";
			bt_io_regulator = "axp806-aldo2";
			bt_io_voltage = <3300000>;
			bt_power_voltage = <330000>;
			bt_rst_n      = <&r_pio PL 2 1 0x1 0x2 0>;
			status        = "okay";
		};

		btlpm: btlpm@0 {
			compatible  = "allwinner,sunxi-btlpm";
			uart_index  = <0x1>;
			bt_wake     = <&r_pio PL 4 1 0x1 0x2 1>;
			bt_hostwake = <&r_pio PL 3 6 0x1 0x2 1>;
			status      = "okay";
		};

		addr_mgt: addr_mgt@0 {
			compatible     = "allwinner,sunxi-addr_mgt";
			type_addr_wifi = <0x0>;
			type_addr_bt   = <0x0>;
			type_addr_eth  = <0x0>;
			status         = "okay";
		};

		uart0: uart@05000000 {
			uart-supply = <&reg_dcdc5>;
		};

		uart1: uart@05000400 {
			status = "okay";
		};

		uart3: uart@05000c00 {
			status = "okay";
		};
	};

	gpu: gpu@0x01800000 {
			gpu_idle = <0>;
			dvfs_status = <1>;
			pll_rate = <504000>;
			operating-points = <
				 /* KHz   uV */
				504000 950000
				472500 950000
				441000 950000
				252000 950000
			 >;
	};

	swd {
		compatible = "creality,swd";
		swdio = <&r_pio PL 8 1 0x1 0x2 1>;
		swclk = <&r_pio PL 9 1 0x1 0x2 1>;
		nrst = <&pio PD 22 1 0x1 0x2 1>;
		status = "okay";
	};
